{
    "AgentConfig": {
        "tool_call_timeout": 30,
        "max_steps": 5,
        "request_limit": 0,
        "total_tokens_limit": 0,
        "memory_results_limit": 5,
        "memory_similarity_threshold": 0.5
    },
    "LLM": {
        "provider": "openai",
        "model": "gpt-4.1",
        "temperature": 0.5,
        "max_tokens": 5000,
