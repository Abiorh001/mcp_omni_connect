        "provider": "openai",
        "model": "text-embedding-3-small",
        "dimensions": 1536,
        "encoding_format": "float",
        "timeout": 600
    },
    "mcpServers": {
        "filesystem-server": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "/home/abiorh/Desktop",
                "/home/abiorh/ai/"
            ]
        }
    }
}
